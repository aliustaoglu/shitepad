{
  "name": "shitepad",
  "version": "0.0.1",
  "description": "Minimal notepad for macos",
  "author": "Cuneyt Aliustaoglu",
  "homepage": "https://github.com/aliustaoglu/shitepad",
  "repository": {
    "type": "git",
    "url": "https://github.com/aliustaoglu/shitepad.git"
  },
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "start": "npm run dev",
    "prod": "webpack --config webpack.build.config.js && electron --noDevServer .",
    "dev": "webpack-dev-server --port 3007 --hot --host 0.0.0.0 --config=./webpack.dev.config.js",
    "build": "webpack --config webpack.build.config.js",
    "package": "webpack --config webpack.build.config.js",
    "postpackage": "electron-packager ./ --out=./builds"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^0.28.11",
    "electron": "^1.7.8",
    "electron-packager": "^9.1.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.28.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "style-loader": "^0.19.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "antd": "^3.5.0"
  }
